{
  "name": "bites",
  "private": true,
  "engines": {
    "node": "18"
  },
  "packageManager": "pnpm@7.31.0",
  "scripts": {
    "build": "pnpm with-env turbo build --filter=@bites/web...",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "db:migrate": "turbo db:migrate",
    "db:push": "turbo db:push db:migrate",
    "dev": "pnpm with-env turbo dev --parallel",
    "format": "prettier --write \"**/*.{js,js,js,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "turbo lint && manypkg check",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "typecheck": "turbo typecheck",
    "with-env": "dotenv -e .env --"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@types/prettier": "^2.7.2",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-bites": "0.0.0",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "turbo": "^1.8.5",
    "typescript": "^5.0.3"
  }
}
